#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here
puts "Welcome to Tic Tac Toe!"
board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]
puts "Where would you like to go?"

index = input_to_index(gets.strip)
index = gets.to_i - 1

def input_to_index(string)
  string.to_i - 1
end



input_to_index(index)
move(board, index, user_marker = "X")
display_board(board)
